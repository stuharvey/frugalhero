{
  "name": "obscurehackathonproject",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prestart": "eslint --fix server.js",
    "start": "node server.js",
    "dev-watch": "DEBUG=express:*,hacktheplanet:* nodemon -x npm start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "debug-logger": "^0.4.1",
    "express": "^4.13.4",
    "jsonfile": "^2.2.3",
    "node-rest-client": "^1.8.0",
    "superagent": "^1.8.0-beta.1",
    "sync-request": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^2.2.0",
    "nodemon": "^1.9.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "complexity": [
        2,
        15
      ],
      "max-len": [
        1,
        80
      ],
      "no-trailing-spaces": 2,
      "brace-style": 2,
      "indent": [
        2,
        2
      ],
      "eol-last": 2,
      "eqeqeq": 2,
      "no-unused-vars": 0
    },
    "extends": "eslint:recommended"
  },
  "optionalDependencies": {
    "http-sync-win": "0.0.7"
  }
}
